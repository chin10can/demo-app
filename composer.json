        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "guzzlehttp/psr7": "^2",
        "laminas/laminas-diactoros": "^2.9",
        "php-di/php-di": "^6.3",
        "illuminate/database": "^9.6",
        "illuminate/pagination": "^9.6",
        "league/event": "^3.0",
        "shipfusion/shipfusion": "dev-main"
    },
    "autoload": {
